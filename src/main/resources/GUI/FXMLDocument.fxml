                <Label prefWidth="60" text="" textFill="#fefefe" />
                <Slider fx:id="Slider5" max="1" min="0.00025" orientation="VERTICAL" prefWidth="60" showTickLabels="true" showTickMarks="true" />
                <Label prefWidth="60" text="" textFill="#fefefe" />
